<h2>presentations</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Graoupack.Models.Presentation.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//graoupack/views/presentation/list',{presentations: presentations})%>
	</tbody>
</table>
<h2>New presentation</h2>
<form>
	<% for(var attr in Graoupack.Models.Presentation.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>